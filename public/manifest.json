  "screenshots": [
    {
      "src": "/screenshots/mobile-home.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Home Screen"
    },
    {
      "src": "/screenshots/mobile-scan.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Scan Items"
    },
    {
      "src": "/screenshots/mobile-listing.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "AI-Generated Listings"
    },
    {
      "src": "/screenshots/desktop-dashboard.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard View"
    }
  ],
  "shortcuts": [
    {
      "name": "New Listing",
      "short_name": "New",
      "description": "Create a new listing",
      "url": "/?action=new",
      "icons": [
        {
          "src": "/icons/icon-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Listings",
      "short_name": "Listings",
      "description": "View your listings",
      "url": "/?view=listings",
      "icons": [
        {
          "src": "/icons/icon-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Connect eBay",
      "short_name": "eBay",
      "description": "Connect your eBay account",
      "url": "/auth/ebay/authorize",
      "icons": [
        {
          "src": "/icons/icon-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "features": [
    "ai-generation",
    "multi-platform-posting",
    "offline-mode",
    "camera-capture",
    "smart-clipboard",
    "ebay-oauth"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": [
            "image/jpeg",
            "image/jpg",
            "image/png",
            "image/webp",
            "image/heic"
          ]
        }
      ]
    }
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+quicklist",
      "url": "/?action=%s"
    }
  ]
}
