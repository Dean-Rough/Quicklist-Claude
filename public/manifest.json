  "display": "standalone",
  "background_color": "#f7f2ea",
  "theme_color": "#0f766e",
  "orientation": "portrait-primary",
  "scope": "/",
  "id": "quicklist-ai-pwa",
  "categories": ["productivity", "business", "utilities", "shopping"],
